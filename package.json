{
  "name": "portfolio_react",
  "version": "1.0.0",
  "description": "Andrew Lu Portfolio",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/homemadechowder/Portfolio_React.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/homemadechowder/Portfolio_React/issues"
  },
  "homepage": "https://github.com/homemadechowder/Portfolio_React#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "crypto-js": "^3.1.9-1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
    "jquery": "^3.4.1",
    "mongoose": "^5.3.16",
    "path": "^0.12.7",
    "qrcode": "^1.4.4",
    "radium": "^0.26.0",
    "react-multi-carousel": "^2.5.2",
    "react-responsive-carousel": "^3.1.51",
    "react-share": "^4.1.0",
    "request": "^2.88.0",
    "uuid": "^7.0.2"
  }
}
